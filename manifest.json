{
    "manifest_version": 3,
    "name": "Modern Youtube",
    "description": "Brings Youtube to life with a modern interface",
    "version": "0.1",
    "content_scripts": [
        {
            "matches": ["*://*.youtube.com/*"],
            "css": ["main.css"],
            "js": ["content.js"]
        }
    ]
}